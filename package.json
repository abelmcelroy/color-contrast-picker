    "@babel/preset-typescript": "^7.18.6",
    "@types/tap": "^15.0.7",
    "babel-loader": "^8.2.4",
    "tap": "^16.3.0",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4",
